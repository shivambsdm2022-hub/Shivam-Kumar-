#include <stdio.h>
#define SIZE 5

int dq[SIZE], front = -1, rear = -1;

void insertFront(int x) {
    if ((front == 0 && rear == SIZE - 1) || front == rear + 1) {
        printf("Deque Overflow\n");
    } else {
        if (front == -1) {
            front = rear = 0;
        } else if (front == 0) {
            front = SIZE - 1;
        } else {
            front--;
        }
        dq[front] = x;
    }
}

void insertRear(int x) {
    if ((front == 0 && rear == SIZE - 1) || front == rear + 1) {
        printf("Deque Overflow\n");
    } else {
        if (rear == -1) {
            front = rear = 0;
        } else if (rear == SIZE - 1) {
            rear = 0;
        } else {
            rear++;
        }
        dq[rear] = x;
    }
}

void display() {
    int i = front;
    while (i != rear) {
        printf("%d ", dq[i]);
        i = (i + 1) % SIZE;
    }
    printf("%d\n", dq[rear]);
}

int main() {
    insertRear(10);
    insertRear(20);
    insertFront(5);
    display();
    return 0;
}
